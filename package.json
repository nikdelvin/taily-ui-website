{
    "name": "taily-ui-website",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "start": "npm run prettier && npm run eslint && astro dev",
        "build": "npm run prettier && npm run eslint && astro check && astro build",
        "preview": "npm run build && astro preview",
        "eslint": "eslint --ext .ts,.tsx,.astro src",
        "prettier": "prettier --write . --plugin=prettier-plugin-astro --plugin=prettier-plugin-tailwindcss",
        "upgrade": "rm package-lock.json && rm -rf node_modules && npm i"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.4",
        "@astrojs/tailwind": "^6.0.2",
        "astro": "^5.14.6",
        "autoprefixer": "^10.4.21",
        "tailwindcss": "^3.4.18",
        "taily-ui": "^1.2.32",
        "typescript": "^5.9.3"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^8.46.1",
        "eslint": "^9.38.0",
        "eslint-plugin-astro": "^1.3.1",
        "prettier": "^3.6.2",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-tailwindcss": "^0.6.14"
    },
    "browserslist": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
    ]
}
